proc ExecutorDispatcher_freezeTask(id:Nat) = 
  (
    sum eid:Nat,taskid:Nat,taskObj:TaskObject,obj:ClassObject.
    synch_call_receive(id,ExecutorDispatcher,obj,freezeTask(eid,taskid,taskObj)).
      synch_call_send(id,ExecutorDispatcher,obj,__taskReceived(taskid,eid)).
      sum reply:Reply,eType:eType.synch_reply_send(id,ExecutorDispatcher,obj,__taskReceived_return(reply,eType)).
      (reply!=OK) -> synch_reply_receive(id,ExecutorDispatcher,obj,freezeTask_return(reply))
      <>
      (
	(eType==None) ->  
	  synch_call_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor(eid,[],false)).
	  sum reply:Reply,tid:Nat.
	    synch_reply_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor_return(reply,tid)).
	  synch_reply_receive(id,ExecutorDispatcher,obj,freezeTask_return(OK))
	<>
	(
	    synch_call_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor(eid,[],false)).
	    sum reply:Reply,tid:Nat.
	      synch_reply_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor_return(reply,tid)).
	    synch_reply_receive(id,ExecutorDispatcher,obj,freezeTask_return(ERROR))
	    +
	    synch_call_send(id,ExecutorDispatcher,obj,__freezeTask(taskid,eType)).
	    sum result:Bool.
	      synch_reply_send(id,ExecutorDispatcher,obj,__freezeTask_return(result)).
	    synch_call_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor(eid,[eType],false)).
	    sum reply:Reply,tid:Nat.
	      synch_reply_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor_return(reply,tid)).
	    synch_reply_receive(id,ExecutorDispatcher,obj,freezeTask_return(OK))
	)
      )
  ).ExecutorDispatcher_freezeTask(id)
;

proc ExecutorDispatcher_taskProcessed(id:Nat) = 
  (
    sum eId:Nat,taskId:Nat,taskObj:TaskObject,obj:ClassObject.
      synch_call_receive(id,ExecutorDispatcher,obj,taskProcessed(eId,taskId,taskObj)).
      synch_call_send(id,ExecutorDispatcher,obj,__taskReceived(taskId,eId)).
      sum reply:Reply,eType:eType.
	synch_reply_send(id,ExecutorDispatcher,obj,__taskReceived_return(reply,eType)).
     (
      (reply!=OK) -> synch_reply_receive(id,ExecutorDispatcher,obj,taskProcessed_return(reply)) 
      <>
      (
	(eType==None) -> 
	    ( 
	      synch_call_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor(eId,[],false)).
	      sum reply:Reply,tid:Nat.
		synch_reply_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor_return(reply,tid)).
	      synch_reply_receive(id,ExecutorDispatcher,obj,taskProcessed_return(OK))  
	    )
	      <>
	      (
		synch_call_send(id,MindCallbacks,__cbHolder,cbTaskProcessed(taskId,taskObj,eType)).
		sum reply1:Reply.synch_reply_send(id,MindCallbacks,__cbHolder,cbTaskProcessed_return(reply1)).
		(
		 (reply1!=OK) -> 
		      ( 
		      synch_call_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor(eId,[],false)).
		      sum reply:Reply,tid:Nat.
			synch_reply_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor_return(reply,tid)).
		      synch_call_send(id,ExecutorDispatcher,obj,removeTaskDispatcher(taskId)).
		      sum reply2:Reply.
			synch_reply_send(id,ExecutorDispatcher,obj,removeTaskDispatcher_return(reply2))
		      .synch_reply_receive(id,ExecutorDispatcher,obj,taskProcessed_return(reply1))
		      )
			<>
			(
			  synch_call_send(id,ExecutorDispatcher,obj,__tasks_update(taskId,taskObj)).
			  synch_reply_send(id,ExecutorDispatcher,obj,__tasks_update_return).
			  synch_call_send(id,ExecutorDispatcher,obj,__dispatchTask(taskId,true)).
			  sum reply2:Reply.
			    synch_reply_send(id,ExecutorDispatcher,obj,__dispatchTask_return(reply2)).
			  synch_call_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor(eId,[eType],false)).
			  sum reply:Reply,tid:Nat.
			    synch_reply_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor_return(reply,tid)).
			  synch_reply_receive(id,ExecutorDispatcher,obj,taskProcessed_return(reply2))
			)

		 )
	      )
       )
      )
  ).ExecutorDispatcher_taskProcessed(id)
;

proc MindCallbacks_cbTaskProcessed(id:Nat) = 
  (
    sum taskid:Nat,taskObj:TaskObject,etype:eType,obj:ClassObject.
      synch_call_receive(id,MindCallbacks,obj,cbTaskProcessed(taskid,taskObj,etype)).
    (
    synch_reply_receive(id,MindCallbacks,obj,cbTaskProcessed_return(OK))
    +
    synch_reply_receive(id,MindCallbacks,obj,cbTaskProcessed_return(ERROR))
    )
  ).MindCallbacks_cbTaskProcessed(id)
;


proc ExecutorDispatcher___taskReceived(id:Nat) =
(
  sum taskid:Nat,eid:Nat,obj:ClassObject.
    synch_call_receive(id,ExecutorDispatcher,obj,__taskReceived(taskid,eid)).
    synch_call_send(id,ExecutorDispatcher,obj,__tasks_get(taskid)).
    sum taskObj:TaskObject.synch_reply_send(id,ExecutorDispatcher,obj,__tasks_get_return(taskObj)).
    (taskObj==NullObj) -> synch_reply_receive(id,ExecutorDispatcher,obj,__taskReceived_return(ERROR,None))
    <>
    (
        synch_call_send(id,ExecutorState,__states,removeTask(taskid,eid)).
	sum result:Bool.synch_reply_send(id,ExecutorState,__states,removeTask_return(result)).
        (!result) -> synch_reply_receive(id,ExecutorDispatcher,obj,__taskReceived_return(OK,None)) <>
	(
	  synch_call_send(id,ExecutorDispatcher,obj,__idMap_get(eid)).
	  sum etypes:List(eType).synch_reply_send(id,ExecutorDispatcher,obj,__idMap_get_return(etypes)).
	  !(eType(taskObj) in etypes) ->
	        synch_call_send(id,ExecutorDispatcher,obj,removeExecutor(eid)).
		synch_reply_send(id,ExecutorDispatcher,obj,removeExecutor_return).
		synch_call_send(id,ExecutorDispatcher,obj,__dispatchTask(taskid,true)).
		sum reply:Reply.
		  synch_reply_send(id,ExecutorDispatcher,obj,__dispatchTask_return(reply)).
		synch_reply_receive(id,ExecutorDispatcher,obj,__taskReceived_return(ERROR,None))
		<>
		 synch_reply_receive(id,ExecutorDispatcher,obj,__taskReceived_return(OK,eType(taskObj)))
	)
    )
).ExecutorDispatcher___taskReceived(id)
;

proc ExecutorDispatcher_removeExecutor(id:Nat) =
(
  sum eid:Nat,obj:ClassObject.synch_call_receive(id,ExecutorDispatcher,obj,removeExecutor(eid)).
    synch_call_send(id,ExecutorDispatcher,obj,__idMap_keys).
    sum eids:List(Nat).
      synch_reply_send(id,ExecutorDispatcher,obj,__idMap_keys_return(eids)).
    !(eid in eids) -> synch_reply_receive(id,ExecutorDispatcher,obj,removeExecutor_return)
      <>
      (
	synch_call_send(id,ExecutorDispatcher,obj,__idMap_pop(eid)).
	sum eTypes:List(eType).synch_reply_send(id,ExecutorDispatcher,obj,__idMap_pop_return(eTypes)).
	loop_removeExecutor4(id,obj,eTypes).
	synch_call_send(id,ExecutorState,__states,removeExecutorStates(eid)).
	sum tasksInExec:List(Nat).
	  synch_reply_send(id,ExecutorState,__states,removeExecutorStates_return(tasksInExec)).
	loop_removeExecutor(id,obj,tasksInExec).
	synch_call_send(id,MindCallbacks,__cbHolder,cbDisconectExecutor(eid)).
	synch_reply_send(id,MindCallbacks,__cbHolder,cbDisconectExecutor_return).
	loop_removeExecutor1(id,obj,eTypes).
	synch_reply_receive(id,ExecutorDispatcher,obj,removeExecutor_return)
      )
).ExecutorDispatcher_removeExecutor(id)
;

proc MindCluster___processTask(id:Nat) = 
(
  sum eId:Nat,taskObj:TaskObject,obj:ClassObject.
    asynch_call_receive(id,MindCluster,obj,msg(ProcessTask,eId, taskObj)). 
    (
    asynch_call_send(id,OptimizationMindHandler,MindHandler,msg(ExecutorError,eId,taskObj))
    +
    asynch_call_send(id,OptimizationMindHandler,MindHandler,msg(TaskError,eId,taskObj))
    +
    asynch_call_send(id,OptimizationMindHandler,MindHandler,msg(TaskFreeze,eId,taskObj))
    +
    ( (eType(taskObj)==JobPath) ->
      
      asynch_call_send(id,OptimizationMindHandler,MindHandler,msg(TaskDone,eId,taskObj(taskID(taskObj),optimizationChain(taskObj),JobPath,statusBundle(Checking,JobSanity))))
     <>
      (eType(taskObj)==JobSanity) ->
      asynch_call_send(id,OptimizationMindHandler,MindHandler,msg(TaskDone,eId,taskObj(taskID(taskObj),optimizationChain(taskObj),JobSanity,statusBundle(Checking,JobScheduling))))
     <>
     (eType(taskObj)==JobScheduling) ->
      asynch_call_send(id,OptimizationMindHandler,MindHandler,msg(TaskDone,eId,taskObj(taskID(taskObj),optimizationChain(taskObj),JobScheduling,statusBundle(Waiting,PilotAgentSubmission))))
     <>
      asynch_call_send(id,OptimizationMindHandler,MindHandler,msg(TaskError,eId,taskObj(taskID(taskObj),optimizationChain(taskObj),eType(taskObj),statusBundle(Failed,FailedMinor))))
    
    )
    )
)
. MindCluster___processTask(id)
;

proc loop_removeExecutor(id:Nat,obj:ClassObject,tasksInExec:List(Nat)) =
  (tasksInExec!=[]) ->
    ( 
	synch_call_send(id,ExecutorDispatcher,obj,__tasks_get(head(tasksInExec))).
	sum taskObj:TaskObject.synch_reply_send(id,ExecutorDispatcher,obj,__tasks_get_return(taskObj)).
	(taskObj!=NullObj && eType(taskObj)!=None) ->
	  synch_call_send(id,ExecutorQueues,__queues,pushTask(eType(taskObj),head(tasksInExec),true)). 
	  synch_reply_send(id,ExecutorQueues,__queues,pushTask_return)
	 <>
	synch_call_send(id,ExecutorDispatcher,obj,__dispatchTask(head(tasksInExec),true)).
	sum reply:Reply.synch_reply_send(id,ExecutorDispatcher,obj,__dispatchTask_return(reply))
    ).loop_removeExecutor(id,obj,tail(tasksInExec))
  <> internal
;

proc loop_removeExecutor1(id:Nat,obj:ClassObject,eTypes:List(eType)) =
  (eTypes!=[]) ->
   synch_call_send(id,ExecutorDispatcher,obj,__fillExecutors(head(eTypes),true)).
   synch_reply_send(id,ExecutorDispatcher,obj,__fillExecutors_return)
   .loop_removeExecutor1(id,obj,tail(eTypes))
  <> internal
;

proc loop_removeExecutor4(id:Nat,obj:ClassObject,eTypes:List(eType)) =
