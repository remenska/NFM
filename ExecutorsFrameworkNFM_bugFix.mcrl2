proc loop_getIdleExecutor(id:Nat,obj:ClassObject,eids:List(Nat)) =
  (
  (eids!=[]) ->
      (
      synch_call_send(id,ExecutorState,obj,freeSlots(head(eids))).
      sum fs:Nat.synch_reply_send(id,ExecutorState,obj,freeSlots_return(fs)).
      
      (fs>0) -> synch_reply_receive(id,ExecutorState,obj,getIdleExecutor_return(head(eids))) 
	<> loop_getIdleExecutor(id,obj,tail(eids))
      )
      <>
      synch_reply_receive(id,ExecutorState,obj,getIdleExecutor_return(0)) % None!
   )
;

proc loop_fillExecutors(id:Nat,obj:ClassObject,eId:Nat,etype:eType) = 
    (
    (eId>0) ->
       synch_call_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor(eId,[etype],false)).
       sum reply:Reply,tid:Nat.synch_reply_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor_return(reply,tid)).
       (!(tid==0) -> 
       synch_call_send(id,ExecutorState,__states,getIdleExecutor(etype)).
       sum eId:Nat.synch_reply_send(id,ExecutorState,__states,getIdleExecutor_return(eId))
      .loop_fillExecutors(id,obj,eId,etype)
      <> internal ) 
	<> internal
    )
;


