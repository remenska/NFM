  synch_reply_receive(id,OptimizationMindHandler,obj,conn_drop_return)
)
.OptimizationMindHandler_conn_drop(id)
;

proc ExecutorDispatcher_addExecutor(id:Nat) =
(  
    sum eid:Nat,eTypes:List(eType),maxTasks:Nat,obj:ClassObject.
      synch_call_receive(id,ExecutorDispatcher,obj,addExecutor(eid,eTypes,maxTasks)).
      synch_call_send(id,ExecutorDispatcher,obj,__idMap_keys).
      sum eids:List(Nat).synch_reply_send(id,ExecutorDispatcher,obj,__idMap_keys_return(eids)).
      (eid in eids) -> synch_reply_receive(id,ExecutorDispatcher,obj,addExecutor_return) <>
      (
	synch_call_send(id,ExecutorDispatcher,obj,__idMap_set(eid,eTypes)).
	  synch_reply_send(id,ExecutorDispatcher,obj,__idMap_set_return).
	synch_call_send(id,ExecutorState,__states,addExecutorState(eid,eTypes,maxTasks)).
	synch_reply_send(id,ExecutorState,__states,addExecutorState_return).
	loop_addExecutor(id,obj,eTypes).
	loop_addExecutor2(id,obj,eTypes).
	synch_reply_receive(id,ExecutorDispatcher,obj,addExecutor_return)
      )
).ExecutorDispatcher_addExecutor(id)
;

proc loop_addExecutor(id:Nat,obj:ClassObject,eTypes:List(eType)) =
  (eTypes!=[]) ->
   synch_call_send(id,ExecutorDispatcher,obj,__execTypes_get_eTypes).
   sum etypes:List(eType).synch_reply_send(id,ExecutorDispatcher,obj,__execTypes_get_eTypes_return(etypes)).
   (!(head(eTypes) in etypes) -> 
	(
	  synch_call_send(id,ExecutorDispatcher,obj,__execTypes_add(head(eTypes))).
	  synch_reply_send(id,ExecutorDispatcher,obj,__execTypes_add_return).
	  synch_call_send(id,ExecutorDispatcher,obj,__execTypes_inc(head(eTypes))).
	  synch_reply_send(id,ExecutorDispatcher,obj,__execTypes_inc_return)
	)
      <> internal)
   .loop_addExecutor(id,obj,tail(eTypes))
  <> internal
;

proc loop_addExecutor2(id:Nat,obj:ClassObject,eTypes:List(eType)) =
(eTypes!=[]) ->
  (
   synch_call_send(id,ExecutorDispatcher,obj,__fillExecutors(head(eTypes),true)).
   synch_reply_send(id,ExecutorDispatcher,obj,__fillExecutors_return)
   .loop_addExecutor2(id,obj,tail(eTypes))
   ) <> internal
;

proc ExecutorState_addExecutor(id:Nat) = 
  (
    sum eid:Nat,eTypes:List(eType),maxTasks:Nat,obj:ClassObject.
      synch_call_receive(id,ExecutorState,obj,addExecutorState(eid,eTypes,maxTasks)). 
      synch_call_send(id,ExecutorState,obj,__maxTasks_add(eid,max(1,maxTasks))).
      synch_reply_send(id,ExecutorState,obj,__maxTasks_add_return).
      synch_call_send(id,ExecutorState,obj,__execTasks_keys).
      sum eids:List(Nat).synch_reply_send(id,ExecutorState,obj,__execTasks_keys_return(eids)).
      (!(eid in eids) -> 
	  synch_call_send(id,ExecutorState,obj,__execTasks_addeid(eid)).
	  synch_reply_send(id,ExecutorState,obj,__execTasks_addeid_return) 
	  <> internal)
    .loop_addExecutor1(id,obj,eTypes,eid)
    .synch_reply_receive(id,ExecutorState,obj,addExecutorState_return)
  )
  .ExecutorState_addExecutor(id)
;

proc loop_addExecutor1(id:Nat,obj:ClassObject,etypes:List(eType),eid:Nat) = 
  (etypes!=[]) -> 
  (
    synch_call_send(id,ExecutorState,obj,__typeToId_keys).
    sum etypes1:List(eType).synch_reply_send(id,ExecutorState,obj,__typeToId_keys_return(etypes1)).
    (!(head(etypes) in etypes1) -> synch_call_send(id,ExecutorState,obj,__typeToId_addeType(head(etypes))).
    synch_reply_send(id,ExecutorState,obj,__typeToId_addeType_return)
			      <> internal)
	.synch_call_send(id,ExecutorState,obj,__typeToId_addeid(head(etypes),eid))
	.synch_reply_send(id,ExecutorState,obj,__typeToId_addeid_return)
	.loop_addExecutor1(id,obj,tail(etypes),eid)
  ) <> internal
;

proc OptimizationMindHandler_exec_executorConnected(id:Nat) =
  (
      sum trid:Nat,eTypes:List(eType),obj:ClassObject.
      synch_call_receive(id,OptimizationMindHandler,obj,exec_executorConnected(trid,eTypes)).
      synch_call_send(id,OptimizationMindHandler,obj,__loadJobs(eTypes)).
      sum reply:Reply.
	synch_reply_send(id,OptimizationMindHandler,obj,__loadJobs_return(reply)).
      synch_reply_receive(id,OptimizationMindHandler,obj,exec_executorConnected_return(reply))
  )
  .OptimizationMindHandler_exec_executorConnected(id)
;

proc OptimizationMindHandler___loadJobs(id:Nat) = 
  (
      sum etypes:List(eType),obj:ClassObject.
	synch_call_receive(id,OptimizationMindHandler,obj,__loadJobs(etypes)).
      (
      (etypes==[]) ->
	  ( 
	  synch_call_send(id,OptimizationMindHandler,obj,getExecutorsConnected).
	  sum etypes:List(eType).
	    synch_reply_send(id,OptimizationMindHandler,obj,getExecutorsConnected_return(etypes)) 
	  ) <> 
	internal) .
      (etypes==[]) -> synch_reply_receive(id,OptimizationMindHandler,obj,__loadJobs_return(OK)) 
	<> 
	(
	  synch_call_send(id,OptimizationMindHandler,obj,getTaskIds).	
	  sum knownJids:List(Nat).synch_reply_send(id,OptimizationMindHandler,obj,getTaskIds_return(knownJids)).
	  loop___loadJobs(id,obj,[],knownJids).
	  synch_reply_receive(id,OptimizationMindHandler,obj,__loadJobs_return(OK))
	)
  )
  .OptimizationMindHandler___loadJobs(id)
;

proc loop___loadJobs(id:Nat,obj:ClassObject,jidList:List(Nat),knownJids:List(Nat)) =
  (jidList!=[]) ->
   (
     (!(head(jidList) in knownJids) -> 
      (
	synch_call_send(id,OptimizationMindHandler,obj,executeTask(head(jidList),taskObj(head(jidList),[JobPath,JobSanity,JobScheduling],JobPath,statusBundle(Received,JobAccepted)))).
	sum reply:Reply.synch_reply_send(id,OptimizationMindHandler,obj,executeTask_return(reply))
    ) <> internal)
  .loop___loadJobs(id,obj,tail(jidList),knownJids)
  )
   <> 
  internal
;

proc ExecutorDispatcher_getTaskIds(id:Nat) =
  sum obj:ClassObject.synch_call_receive(id,ExecutorDispatcher,obj,getTaskIds).
    synch_call_send(id,ExecutorDispatcher,obj,__tasks_keys).
    sum taskids:List(Nat).synch_reply_send(id,ExecutorDispatcher,obj,__tasks_keys_return(taskids)).
  synch_reply_receive(id,ExecutorDispatcher,obj,getTaskIds_return(taskids))
.ExecutorDispatcher_getTaskIds(id);

proc ExecutorDispatcher_getExecutorsConnected(id:Nat) =
  sum obj:ClassObject.synch_call_receive(id,ExecutorDispatcher,obj,getExecutorsConnected).
    synch_call_send(id,ExecutorDispatcher,obj,__execTypes_getExecutorsConnected).
    sum etypes:List(eType).
      synch_reply_send(id,ExecutorDispatcher,obj,__execTypes_getExecutorsConnected_return(etypes)).
  synch_reply_receive(id,ExecutorDispatcher,obj,getExecutorsConnected_return(etypes))
.ExecutorDispatcher_getExecutorsConnected(id);

proc ExecutorDispatcher___doPeriodicStuff(id:Nat) =

    synch_call_send(id,ExecutorDispatcher,__eDispatch1,__unfreezeTasks(None)).
    synch_reply_send(id,ExecutorDispatcher,__eDispatch1,__unfreezeTasks_return).
ExecutorDispatcher___doPeriodicStuff(id);

