proc ExecutorState_removeExecutor(id:Nat) =
(
      sum eid:Nat,obj:ClassObject.
	synch_call_receive(id,ExecutorState,obj,removeExecutorStates(eid)).
      synch_call_send(id,ExecutorState,obj,__typeToId_keys).
      sum etypes:List(eType).synch_reply_send(id,ExecutorState,obj,__typeToId_keys_return(etypes)).
      loop_removeExecutor2(id,obj,etypes,eid).
      synch_call_send(id,ExecutorState,obj,__execTasks_taskids(eid)).
      sum taskids:List(Nat).synch_reply_send(id,ExecutorState,obj,__execTasks_taskids_return(taskids)).
      loop_removeExecutor3(id,obj,taskids).
      synch_call_send(id,ExecutorState,obj,__execTasks_pop(eid)).
      synch_reply_send(id,ExecutorState,obj,__execTasks_pop_return).
      synch_call_send(id,ExecutorState,obj,__maxTasks_pop(eid)).
      synch_reply_send(id,ExecutorState,obj,__maxTasks_pop_return).
    synch_reply_receive(id,ExecutorState,obj,removeExecutorStates_return(taskids)) 
).ExecutorState_removeExecutor(id)
;

