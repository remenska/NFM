proc ExecutorDispatcher_addTask(id:Nat) = 
(
    sum taskid:Nat,taskObj:TaskObject,obj:ClassObject.
      synch_call_receive(id,ExecutorDispatcher,obj,addTaskDispatcher(taskid,taskObj)).
    synch_call_send(id,ExecutorDispatcher,obj,__addTaskIfNew(taskid,taskObj)).
    sum result:Bool.synch_reply_send(id,ExecutorDispatcher,obj,__addTaskIfNew_return(result)).
    (!result) -> 
	synch_call_send(id,ExecutorDispatcher,obj,__unfreezeTasks(None)).
	synch_reply_send(id,ExecutorDispatcher,obj,__unfreezeTasks_return).
	synch_reply_receive(id,ExecutorDispatcher,obj,addTaskDispatcher_return(OK))
      <>
	synch_call_send(id,ExecutorDispatcher,obj,__dispatchTask(taskid,true)).
	sum reply:Reply.synch_reply_send(id,ExecutorDispatcher,obj,__dispatchTask_return(reply)).
	synch_reply_receive(id,ExecutorDispatcher,obj,addTaskDispatcher_return(reply))
).ExecutorDispatcher_addTask(id)
;


proc ExecutorDispatcher___addTaskIfNew(id:Nat) = 
 (
  sum taskid:Nat,taskObj:TaskObject,obj:ClassObject.
    synch_call_receive(id,ExecutorDispatcher,obj,__addTaskIfNew(taskid,taskObj)).
    synch_call_send(id,ExecutorDispatcher,obj,__tasks_keys).
    sum taskids:List(Nat).synch_reply_send(id,ExecutorDispatcher,obj,__tasks_keys_return(taskids)).
    (taskid in taskids) -> synch_reply_receive(id,ExecutorDispatcher,obj,__addTaskIfNew_return(false))
    <>
    (
      synch_call_send(id,ExecutorDispatcher,obj,__tasks_add(taskObj)).
	synch_reply_send(id,ExecutorDispatcher,obj,__tasks_add_return).
      synch_reply_receive(id,ExecutorDispatcher,obj,__addTaskIfNew_return(true))
    )
  ).ExecutorDispatcher___addTaskIfNew(id)
;

