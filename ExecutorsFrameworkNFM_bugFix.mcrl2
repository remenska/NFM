proc Monitor(b:Bool) = 
	sum taskId:Nat,eType:eType.step1_1(taskId,eType).Monitor(true)
	+
	sum taskId:Nat,eType:eType.step2_1(taskId,eType).Monitor(false)
	+ step3_1(0). b -> found <> Monitor(b);


init hide ({internal},
 allow({internal,synch_call,synch_reply, asynch_call,found,step1,step2,step3
},
comm({
synch_call_send|synch_call_receive->
synch_call,
synch_reply_send|synch_reply_receive->
synch_reply,
asynch_call_send|asynch_call_receive->
asynch_call,
step1_1|step1_2->
step1,
step2_1|step2_2->
step2,
step3_1|step3_2->
step3
}, 
				systemInit(1)

)));

