act found;


act step1_1,step1_2,step1:Nat#eType;
act step2_1,step2_2,step2:Nat#eType;
act step3_1,step3_2,step3:Nat;

map minorStatus2eType:MinorStatus->eType;
var minor:MinorStatus;
eqn
	minorStatus2eType(minor) = if(minor==JobPath,JobPath,
		  if(minor==JobSanity,JobSanity,
		    if(minor==JobScheduling,JobScheduling,
		      if(minor==InputData,InputData,None
			))));

proc ExecutorDispatcher_removeTask(id:Nat) =
(
    
    sum taskId:Nat,obj:ClassObject.synch_call_receive(id,ExecutorDispatcher,obj,removeTaskDispatcher(taskId)).
    synch_call_send(id,ExecutorDispatcher,obj,__tasks_pop(taskId)).
    synch_reply_send(id,ExecutorDispatcher,obj,__tasks_pop_return). 
    synch_call_send(id,ExecutorState,__states, getExecutorOfTask(taskId)). 
    sum eId:Nat.synch_reply_send(id,ExecutorState,__states,getExecutorOfTask_return(eId)).
    
    ( synch_call_send(id,ExecutorQueues,__queues,deleteTask(taskId)).
      synch_reply_send(id,ExecutorQueues,__queues,deleteTask_return).
      synch_call_send(id,ExecutorState,__states,removeTask(taskId,0)).
      sum result:Bool.synch_reply_send(id,ExecutorState,__states,removeTask_return(result)).
      synch_call_send(id,ExecutorDispatcher,obj,__taskFreezer_index(taskId)).
      sum ind:Int.synch_reply_send(id,ExecutorDispatcher,obj,__taskFreezer_index_return(ind)).
      ((ind>0) -> (
		  synch_call_send(id,ExecutorDispatcher,obj,__taskFreezer_pop(Int2Nat(ind))).
		  synch_reply_send(id,ExecutorDispatcher,obj,__taskFreezer_pop_return)
		    ) <> internal).
      ((eId>0) -> 
      synch_call_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor(eId,[],true)).
      sum reply:Reply,tid:Nat.synch_reply_send(id,ExecutorDispatcher,obj,__sendTaskToExecutor_return(reply,tid)) 
      .synch_reply_receive(id,ExecutorDispatcher,obj,removeTaskDispatcher_return(reply)) 
      <> synch_reply_receive(id,ExecutorDispatcher,obj,removeTaskDispatcher_return(OK))
      )
    )

)
. ExecutorDispatcher_removeTask(id)
;
  
proc ExecutorState_getExecutorOfTask(id:Nat) = 

(
	sum taskId:Nat,obj:ClassObject.synch_call_receive(id,ExecutorState,obj,getExecutorOfTask(taskId)).
	  synch_call_send(id,ExecutorState,obj,__taskInExec_get(taskId)).
	  sum eid:Int.synch_reply_send(id,ExecutorState,obj,__taskInExec_get_return(eid)).
	  (eid>0) -> synch_reply_receive(id,ExecutorState,obj,getExecutorOfTask_return(Int2Nat(eid)))
	    <>
		    synch_reply_receive(id,ExecutorState,obj,getExecutorOfTask_return(0)) 

)
. ExecutorState_getExecutorOfTask(id)
;

proc ExecutorQueues_deleteTask(id:Nat) = 
(
    sum taskId:Nat,obj:ClassObject. synch_call_receive(id,ExecutorQueues,obj,deleteTask(taskId)).
    synch_call_send(id,ExecutorQueues,obj,__taskInQueue_get(taskId)).
    sum etype:eType.synch_reply_send(id,ExecutorQueues,obj,__taskInQueue_get_return(etype)).
    
      synch_call_send(id,ExecutorQueues,obj,__taskInQueue_del(taskId)).
      synch_reply_send(id,ExecutorQueues,obj,__taskInQueue_del_return).
      
      synch_call_send(id,ExecutorQueues,obj,__queues_index(etype,taskId)).
      sum iPos:Int.synch_reply_send(id,ExecutorQueues,obj,__queues_index_return(iPos)).
      ((iPos!=-1) -> 
	  synch_call_send(id,ExecutorQueues,obj,__queues_delAtPos(etype,Int2Nat(iPos))).
	  synch_reply_send(id,ExecutorQueues,obj,__queues_delAtPos_return)
	  <> internal)
    .synch_reply_receive(id,ExecutorQueues,obj,deleteTask_return)

)
.ExecutorQueues_deleteTask(id)
;

proc ExecutorState_removeTask(id:Nat) = 
(
    sum taskId:Nat,eId:Nat,obj:ClassObject.synch_call_receive(id,ExecutorState,obj,removeTask(taskId,eId)).
    (
	    synch_call_send(id,ExecutorState,obj,__taskInExec_get(taskId)). 
	    sum eId:Int.synch_reply_send(id,ExecutorState,obj,__taskInExec_get_return(eId)).
	    (eId>0)->
	      (
	      synch_call_send(id,ExecutorState,obj,__execTasks_removetaskid(Int2Nat(eId),taskId)).
	      synch_reply_send(id,ExecutorState,obj,__execTasks_removetaskid_return).
	      synch_call_send(id,ExecutorState,obj,__taskInExec_pop(taskId)).
	      synch_reply_send(id,ExecutorState,obj,__taskInExec_pop_return).
	      synch_reply_receive(id,ExecutorState,obj,removeTask_return(true))
		)
		<> 
	      (
	      synch_call_send(id,ExecutorState,obj,__taskInExec_pop(taskId)).
	      synch_reply_send(id,ExecutorState,obj,__taskInExec_pop_return).
	      synch_reply_receive(id,ExecutorState,obj,removeTask_return(false))
	      )

      )
)
.ExecutorState_removeTask(id)
;

proc ExecutorDispatcher___sendTaskToExecutor(id:Nat) = 
(
    sum eId:Nat,eTypes:List(eType),checkIdle:Bool,obj:ClassObject.
    synch_call_receive(id,ExecutorDispatcher,obj,__sendTaskToExecutor(eId,eTypes,checkIdle)). 
    synch_call_send(id,ExecutorState,__states,freeSlots(eId)). 
    sum noSlots:Nat.synch_reply_send(id,ExecutorState,__states,freeSlots_return(noSlots)).
    (checkIdle && noSlots==0) ->
      synch_reply_receive(id,ExecutorDispatcher,obj,__sendTaskToExecutor_return(OK,0)).step3_2(0) <> 
	    ( %BUGFIX when trying to fill an executor, the dispatcher will look at all the possible types always
		synch_call_send(id,ExecutorDispatcher,obj,__idMap_get(eId)).
		sum eTypes:List(eType).synch_reply_send(id,ExecutorDispatcher,obj,__idMap_get_return(eTypes)).
		synch_call_send(id,ExecutorQueues,__queues,popTask(eTypes)).
		sum taskId:Nat,eType:eType.synch_reply_send(id,ExecutorQueues,__queues,popTask_return(taskId,eType)).
		((taskId!=0)-> step2_2(taskId,eType) <> internal).
		(taskId==0) ->
		synch_reply_receive(id,ExecutorDispatcher,obj,__sendTaskToExecutor_return(OK,0)).step3_2(0) <> 
			(
			synch_call_send(id,ExecutorState,__states,addTask(eId,taskId)).
			synch_reply_send(id,ExecutorState,__states,addTask_return).
			synch_call_send(id,ExecutorDispatcher,obj,__msgTaskToExecutor(taskId,eId,eType)). 
			sum reply:Reply.synch_reply_send(id,ExecutorDispatcher,obj,__msgTaskToExecutor_return(reply)).
			(reply!=OK) ->
			synch_call_send(id,ExecutorQueues,__queues,pushTask(eType,taskId,true)). 
			synch_reply_send(id,ExecutorQueues,__queues,pushTask_return).
			synch_call_send(id,ExecutorState,__states,removeTask(taskId,0)).
			sum result:Bool.synch_reply_send(id,ExecutorState,__states,removeTask_return(result)).
			synch_reply_receive(id,ExecutorDispatcher,obj,__sendTaskToExecutor_return(reply,0))
			<>  synch_reply_receive(id,ExecutorDispatcher,obj,__sendTaskToExecutor_return(OK,taskId))
			)
	    )

)
.ExecutorDispatcher___sendTaskToExecutor(id)
;

ExecutorState_freeSlots(id:Nat) = 
(
    sum eId:Nat,obj:ClassObject.synch_call_receive(id,ExecutorState,obj,freeSlots(eId)).
      synch_call_send(id,ExecutorState,obj,__maxTasks_get(eId)).
      sum maxtasks:Nat.synch_reply_send(id,ExecutorState,obj,__maxTasks_get_return(maxtasks)).
      synch_call_send(id,ExecutorState,obj,__execTasks_taskids(eId)).
      sum taskids:List(Nat).synch_reply_send(id,ExecutorState,obj,__execTasks_taskids_return(taskids)).
      (maxtasks==0) -> synch_reply_receive(id,ExecutorState,obj,freeSlots_return(0))
	    <>
		  synch_reply_receive(id,ExecutorState,obj,freeSlots_return(Int2Nat(maxtasks-#taskids)))
)
.ExecutorState_freeSlots(id)
;

ExecutorQueues_popTask(id:Nat) = 
(
    sum eTypes:List(eType),obj:ClassObject.synch_call_receive(id,ExecutorQueues,obj,popTask(eTypes)). 
    loop_popTask(id,obj,eTypes)
)
.ExecutorQueues_popTask(id)
;

proc loop_popTask(id:Nat,obj:ClassObject,eTypes:List(eType)) = 
  (eTypes!=[]) ->
    synch_call_send(id,ExecutorQueues,obj,__queues_pop(head(eTypes),0)).
    sum taskid:Int.synch_reply_send(id,ExecutorQueues,obj,__queues_pop_return(taskid)).
    ((taskid!=-1) ->
      synch_call_send(id,ExecutorQueues,obj,__taskInQueue_del(Int2Nat(taskid))).
      synch_reply_send(id,ExecutorQueues,obj,__taskInQueue_del_return).
      synch_reply_receive(id,ExecutorQueues,obj,popTask_return(Int2Nat(taskid),head(eTypes)))
      <>
      loop_popTask(id,obj,tail(eTypes))
      )
    <>
    synch_reply_receive(id,ExecutorQueues,obj,popTask_return(0,None))
;

ExecutorState_addTask(id:Nat) = 
(
sum eId:Nat,taskId:Nat,obj:ClassObject.
    synch_call_receive(id,ExecutorState,obj,addTask(eId,taskId)).
  synch_call_send(id,ExecutorState,obj,__taskInExec_set(taskId,eId)).
  synch_reply_send(id,ExecutorState,obj,__taskInExec_set_return).
  synch_call_send(id,ExecutorState,obj,__execTasks_addtaskid(eId,taskId)).
  synch_reply_send(id,ExecutorState,obj,__execTasks_addtaskid_return).
  synch_reply_receive(id,ExecutorState,obj,addTask_return)
)
.ExecutorState_addTask(id)
;

proc ExecutorDispatcher___msgTaskToExecutor(id:Nat) =
(
    sum taskId:Nat,eId:Nat,eType:eType,obj:ClassObject.
      synch_call_receive(id,ExecutorDispatcher,obj,__msgTaskToExecutor(taskId,eId,eType)). 
    synch_call_send(id,ExecutorDispatcher,obj,__tasks_get(taskId)).
    sum taskObj:TaskObject.synch_reply_send(id,ExecutorDispatcher,obj,__tasks_get_return(taskObj)).
    (taskObj==NullObj) -> synch_reply_receive(id,ExecutorDispatcher,obj,__msgTaskToExecutor_return(ERROR))
     <>
      synch_call_send(id,MindCallbacks,__cbHolder,cbSendTask(taskId,taskObj,eId,eType)).
      sum reply:Reply.synch_reply_send(id,MindCallbacks,__cbHolder,cbSendTask_return(reply)).
      (reply==OK) -> 
      synch_reply_receive(id,ExecutorDispatcher,obj,__msgTaskToExecutor_return(OK)) 
      <>
      synch_reply_receive(id,ExecutorDispatcher,obj,__msgTaskToExecutor_return(ERROR))
)
.ExecutorDispatcher___msgTaskToExecutor(id)
;

proc ExecutorQueues_pushTask(id:Nat) = 
(
    sum eType:eType,taskId:Nat,ahead:Bool,obj:ClassObject.
	synch_call_receive(id,ExecutorQueues,obj,pushTask(eType,taskId,ahead)). step1_2(taskId,eType).
      synch_call_send(id,ExecutorQueues,obj,__taskInQueue_keys).
      sum taskids:List(Nat).synch_reply_send(id,ExecutorQueues,obj,__taskInQueue_keys_return(taskids)).
      (taskId in taskids) -> synch_reply_receive(id,ExecutorQueues,obj,pushTask_return)
      <>
      (
	synch_call_send(id,ExecutorQueues,obj,__queues_keys).
	sum etypes:List(eType).synch_reply_send(id,ExecutorQueues,obj,__queues_keys_return(etypes)).
	(!(eType in etypes) ->
	    synch_call_send(id,ExecutorQueues,obj,__queues_addetype(eType)).
	    synch_reply_send(id,ExecutorQueues,obj,__queues_addetype_return)
	    <>
	    internal
	 )   
	.(
	  (ahead) -> synch_call_send(id,ExecutorQueues,obj,__queues_insert(eType,taskId)).
		      synch_reply_send(id,ExecutorQueues,obj,__queues_insert_return)
	    <>
		  synch_call_send(id,ExecutorQueues,obj,__queues_append(eType,taskId)).
		  synch_reply_send(id,ExecutorQueues,obj,__queues_append_return)
         )
      ).
	synch_call_send(id,ExecutorQueues,obj,__taskInQueue_set(taskId,eType)).
	synch_reply_send(id,ExecutorQueues,obj,__taskInQueue_set_return).
	synch_reply_receive(id,ExecutorQueues,obj,pushTask_return)
)
.ExecutorQueues_pushTask(id)
;

proc MindCallbacks_cbSendTask(id:Nat) = 
(
  sum taskId:Nat,taskObj:TaskObject,eId:Nat,eType:eType,obj:ClassObject. 
      synch_call_receive(id,MindCallbacks,obj,cbSendTask(taskId,taskObj,eId,eType)). 
  (
  synch_reply_receive(id,MindCallbacks,obj,cbSendTask_return(OK)).
  asynch_call_send(id,MindCluster,mindCluster,msg(ProcessTask,eId, taskObj))
  +
  synch_reply_receive(id,MindCallbacks,obj,cbSendTask_return(ERROR))

  )
)
.MindCallbacks_cbSendTask(id)
;

proc ExecutorDispatcher___fillExecutors(id:Nat) = 
(
  sum etype:eType,defrozeIfNeeded:Bool,obj:ClassObject.
    synch_call_receive(id,ExecutorDispatcher,obj,__fillExecutors(etype,defrozeIfNeeded)).
  (defrozeIfNeeded ->
   synch_call_send(id,ExecutorDispatcher,obj,__unfreezeTasks(etype)).
   synch_reply_send(id,ExecutorDispatcher,obj,__unfreezeTasks_return)
   <> internal
  ).
  synch_call_send(id,ExecutorState,__states,getIdleExecutor(etype)).
  sum eId:Nat.synch_reply_send(id,ExecutorState,__states,getIdleExecutor_return(eId))
  .loop_fillExecutors(id,obj,eId,etype).
  synch_reply_receive(id,ExecutorDispatcher,obj,__fillExecutors_return)
).ExecutorDispatcher___fillExecutors(id)
;

proc ExecutorDispatcher___unfreezeTasks(id:Nat) =
  sum etype:eType,obj:ClassObject.
    synch_call_receive(id,ExecutorDispatcher,obj,__unfreezeTasks(etype)).
  loop___unfreezeTasks(id,obj,0,etype)
.ExecutorDispatcher___unfreezeTasks(id)
;

proc loop___unfreezeTasks(id:Nat,obj:ClassObject,iP:Nat,etype:eType) = 
  synch_call_send(id,ExecutorDispatcher,obj,__taskFreezer_len).
  sum len:Nat.synch_reply_send(id,ExecutorDispatcher,obj,__taskFreezer_len_return(len)).
  (iP<len) ->
    (
      synch_call_send(id,ExecutorDispatcher,obj,__taskFreezer_get(iP)).
      sum taskid:Int.synch_reply_send(id,ExecutorDispatcher,obj,__taskFreezer_get_return(taskid)).
      (taskid==-1) -> synch_reply_receive(id,ExecutorDispatcher,obj,__unfreezeTasks_return)
      <>
      (
	synch_call_send(id,ExecutorDispatcher,obj,__tasks_get(Int2Nat(taskid))).
	sum eTask:TaskObject.synch_reply_send(id,ExecutorDispatcher,obj,__tasks_get_return(eTask)).
	(eTask==NullObj) -> synch_call_send(id,ExecutorDispatcher,obj,__taskFreezer_pop(iP)).
			      synch_reply_send(id,ExecutorDispatcher,obj,__taskFreezer_pop_return).
			    loop___unfreezeTasks(id,obj,iP,etype)
	  <>
	  ((etype!=None && !(etype==eType(eTask))) -> loop___unfreezeTasks(id,obj,succ(iP),etype)
	    <>
	      (
	      synch_call_send(id,ExecutorDispatcher,obj,__taskFreezer_pop(iP)).
	      synch_reply_send(id,ExecutorDispatcher,obj,__taskFreezer_pop_return).
	      synch_call_send(id,ExecutorDispatcher,obj,__dispatchTask(Int2Nat(taskid),false)). 
	      sum reply:Reply.synch_reply_send(id,ExecutorDispatcher,obj,__dispatchTask_return(reply)).
	      loop___unfreezeTasks(id,obj,iP,etype)
	      )
	    )
      )
    )
    <>  synch_reply_receive(id,ExecutorDispatcher,obj,__unfreezeTasks_return)
;

proc ExecutorDispatcher___dispatchTask(id:Nat) =
 (
    sum taskid:Nat,defrozeIfNeeded:Bool,obj:ClassObject.
      synch_call_receive(id,ExecutorDispatcher,obj,__dispatchTask(taskid,defrozeIfNeeded)).
    synch_call_send(id,ExecutorState,__states,getExecutorOfTask(taskid)). 
    sum eid:Nat.synch_reply_send(id,ExecutorState,__states,getExecutorOfTask_return(eid)).
  (
    (eid>0) ->
      synch_reply_receive(id,ExecutorDispatcher,obj,__dispatchTask_return(OK))
      <>
      (
	    synch_call_send(id,ExecutorDispatcher,obj,__removeFromFreezer(taskid)). 
	    sum result:Bool.synch_reply_send(id,ExecutorDispatcher,obj,__removeFromFreezer_return(result)).
	    synch_call_send(id,ExecutorDispatcher,obj,__getNextExecutor(taskid)).
	    sum reply:Reply,etype:eType.synch_reply_send(id,ExecutorDispatcher,obj,__getNextExecutor_return(reply,etype)). 
	    (reply!=OK) ->
	      (
		
		synch_call_send(id,ExecutorDispatcher,obj,__freezeTask(taskid,etype)).
		sum result:Bool.synch_reply_send(id,ExecutorDispatcher,obj,__freezeTask_return(result)).
		((result) -> synch_reply_receive(id,ExecutorDispatcher,obj,__dispatchTask_return(OK)) 
			    <> synch_reply_receive(id,ExecutorDispatcher,obj,__dispatchTask_return(reply)))
		+ 
		synch_call_send(id,ExecutorDispatcher,obj,getTask(taskid)).
		sum taskObj:TaskObject.synch_reply_send(id,ExecutorDispatcher,obj,getTask_return(taskObj)).
		synch_call_send(id,ExecutorDispatcher,obj,removeTaskDispatcher(taskid)).
		sum reply1:Reply.synch_reply_send(id,ExecutorDispatcher,obj,removeTaskDispatcher_return(reply1)).
		synch_reply_receive(id,ExecutorDispatcher,obj,__dispatchTask_return(ERROR))
	    ) 
	    <>
	     (
	      (etype==None) ->
		(
		  synch_call_send(id,ExecutorDispatcher,obj,removeTaskDispatcher(taskid)).
		  sum reply2:Reply.synch_reply_send(id,ExecutorDispatcher,obj,removeTaskDispatcher_return(reply2)).
		  synch_reply_receive(id,ExecutorDispatcher,obj,__dispatchTask_return(reply2))
		) 
		<>
		(
		  synch_call_send(id,ExecutorDispatcher,obj,__execTypes_get_eTypes).
		  sum exectypes:List(eType).synch_reply_send(id,ExecutorDispatcher,obj,__execTypes_get_eTypes_return(exectypes)).
		  (!(etype in exectypes) ->
		    (
		    synch_call_send(id,ExecutorDispatcher,obj,__freezeTask(taskid,etype)).
		    sum result1:Bool.synch_reply_send(id,ExecutorDispatcher,obj,__freezeTask_return(result1)).
		    synch_reply_receive(id,ExecutorDispatcher,obj,__dispatchTask_return(OK))
		    +
		    synch_call_send(id,ExecutorDispatcher,obj,removeTaskDispatcher(taskid)).
		    sum reply1:Reply.synch_reply_send(id,ExecutorDispatcher,obj,removeTaskDispatcher_return(reply1)).
		    synch_reply_receive(id,ExecutorDispatcher,obj,__dispatchTask_return(reply1))
		    )
		    <> (
			synch_call_send(id,ExecutorQueues,__queues,pushTask(etype,taskid,false)). 
			synch_reply_send(id,ExecutorQueues,__queues, pushTask_return).
			synch_call_send(id,ExecutorDispatcher,obj,__fillExecutors(etype,defrozeIfNeeded)).
			synch_reply_send(id,ExecutorDispatcher,obj,__fillExecutors_return).
			synch_reply_receive(id,ExecutorDispatcher,obj,__dispatchTask_return(OK))
			)
		  )
	      )
	    )  
      )
    )
).ExecutorDispatcher___dispatchTask(id)
;

proc ExecutorDispatcher___getNextExecutor(id:Nat) =
 (
    sum taskid:Nat,obj:ClassObject.synch_call_receive(id,ExecutorDispatcher,obj,__getNextExecutor(taskid)).
    synch_call_send(id,ExecutorDispatcher,obj,__tasks_get(taskid)).
    sum taskObj:TaskObject.synch_reply_send(id,ExecutorDispatcher,obj,__tasks_get_return(taskObj)).
    (taskObj==NullObj) -> synch_reply_receive(id,ExecutorDispatcher,obj,__getNextExecutor_return(ERROR,None))
	<>
      (
      synch_call_send(id,MindCallbacks,__cbHolder,cbDispatch(taskid, taskObj,optimizationChain(taskObj))).
      sum reply:Reply,etype:eType.
	  synch_reply_send(id,MindCallbacks,__cbHolder,cbDispatch_return(reply,etype)).
      (reply!=OK) -> synch_reply_receive(id,ExecutorDispatcher,obj,__getNextExecutor_return(ERROR,None))
	<>
	synch_call_send(id,ExecutorDispatcher,obj, __tasks_update(taskid,taskObj(taskID(taskObj),optimizationChain(taskObj),etype,status(taskObj)))).
	synch_reply_send(id,ExecutorDispatcher,obj,__tasks_update_return).
	synch_reply_receive(id,ExecutorDispatcher,obj,__getNextExecutor_return(OK,etype))
      )
  ).ExecutorDispatcher___getNextExecutor(id)
;

proc ExecutorDispatcher_getTask(id:Nat) =
  sum taskid:Nat,obj:ClassObject.synch_call_receive(id,ExecutorDispatcher,obj,getTask(taskid)).
  synch_call_send(id,ExecutorDispatcher,obj,__tasks_get(taskid))
  .sum taskObj:TaskObject.synch_reply_send(id,ExecutorDispatcher,obj,__tasks_get_return(taskObj)).
  synch_reply_receive(id,ExecutorDispatcher,obj,getTask_return(taskObj))
.ExecutorDispatcher_getTask(id)
;

proc ExecutorDispatcher___freezeTask(id:Nat) =
 (
      sum taskid:Nat,eType:eType,obj:ClassObject.
      synch_call_receive(id,ExecutorDispatcher,obj,__freezeTask(taskid,eType)).
	synch_call_send(id,ExecutorDispatcher,obj,__taskFreezer_check(taskid)).
	sum isIn:Bool.synch_reply_send(id,ExecutorDispatcher,obj,__taskFreezer_check_return(isIn)).
	isIn -> synch_reply_receive(id,ExecutorDispatcher,obj,__freezeTask_return(false)) <>
	(
	  synch_call_send(id,ExecutorDispatcher,obj,__tasks_get(taskid))
	  .sum taskObj:TaskObject.synch_reply_send(id,ExecutorDispatcher,obj,__tasks_get_return(taskObj)).
	  (taskObj==NullObj) -> synch_reply_receive(id,ExecutorDispatcher,obj,__freezeTask_return(false)) <>
	    (
	      synch_call_send(id,ExecutorDispatcher,obj,__taskFreezer_append(taskid)).
	      synch_reply_send(id,ExecutorDispatcher,obj,__taskFreezer_append_return).
	      synch_reply_receive(id,ExecutorDispatcher,obj,__freezeTask_return(true))
	      +
	      synch_call_send(id,ExecutorDispatcher,obj,removeTaskDispatcher(taskid)).
	      sum reply:Reply.synch_reply_send(id,ExecutorDispatcher,obj,removeTaskDispatcher_return(reply)).
	      synch_reply_receive(id,ExecutorDispatcher,obj,__freezeTask_return(false))
	    )
	)
  )
.ExecutorDispatcher___freezeTask(id)
;

proc MindCallbacks_cbDispatch(id:Nat) = 
  sum taskid:Nat,taskObj:TaskObject, executedPath:List(eType),obj:ClassObject.
    synch_call_receive(id, MindCallbacks,obj,cbDispatch(taskid,taskObj,executedPath)).
    synch_call_send(id,OptimizationMindHandler,MindHandler, exec_dispatch(taskid,taskObj,executedPath)).
    sum reply:Reply,nextExecutor:eType.
    synch_reply_send(id,OptimizationMindHandler,MindHandler,exec_dispatch_return(reply,nextExecutor)).
  synch_reply_receive(id,MindCallbacks,obj, cbDispatch_return(reply,nextExecutor))
.MindCallbacks_cbDispatch(id)
;

proc OptimizationMindHandler_exec_dispatch(id:Nat) = 
  (
      sum jid:Nat,jobState:TaskObject,pathExecuted:List(eType),obj:ClassObject.
      synch_call_receive(id, OptimizationMindHandler,obj,exec_dispatch(jid,jobState,pathExecuted)).

      (status(status(jobState))==Received) -> 
	    synch_reply_receive(id,OptimizationMindHandler,obj,exec_dispatch_return(OK,JobPath))
	  <>
	(status(status(jobState))!=Checking) -> 
	  synch_reply_receive(id,OptimizationMindHandler,obj,exec_dispatch_return(OK,None)) 
	<>
	(minorStatus(status(jobState)) in optimizationChain(jobState)) ->
	    synch_reply_receive(id,OptimizationMindHandler,obj,exec_dispatch_return(OK,minorStatus2eType(minorStatus(status(jobState)))))
	    <>
	         synch_reply_receive(id,OptimizationMindHandler,obj,exec_dispatch_return(ERROR,None))
     
  )

.OptimizationMindHandler_exec_dispatch(id)
;
 
proc ExecutorDispatcher___removeFromFreezer(id:Nat) = 
  (
      sum taskid:Nat,obj:ClassObject.
	synch_call_receive(id,ExecutorDispatcher,obj,__removeFromFreezer(taskid)).
	synch_call_send(id,ExecutorDispatcher,obj,__taskFreezer_index(taskid)).
	sum iP:Int.synch_reply_send(id,ExecutorDispatcher,obj,__taskFreezer_index_return(iP)).
	(iP==-1) -> synch_reply_receive(id,ExecutorDispatcher,obj,__removeFromFreezer_return(false))
	<>
	(
	  synch_call_send(id,ExecutorDispatcher,obj,__taskFreezer_pop(Int2Nat(iP))).
	  synch_reply_send(id,ExecutorDispatcher,obj,__taskFreezer_pop_return).
	  synch_call_send(id,ExecutorDispatcher,obj,__tasks_get(taskid)).
	  sum taskObj:TaskObject.synch_reply_send(id,ExecutorDispatcher,obj,__tasks_get_return(taskObj)).
	  (taskObj==NullObj) -> synch_call_receive(id,ExecutorDispatcher,obj,__removeFromFreezer_return(false))
	    <>
	  synch_reply_receive(id,ExecutorDispatcher,obj,__removeFromFreezer_return(true))
	)
  )
.ExecutorDispatcher___removeFromFreezer(id)
;

proc ExecutorState_getIdleExecutor(id:Nat) =
